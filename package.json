		"test-e2e": "node --run pre-test && node --experimental-strip-types --disable-warning=ExperimentalWarning node_modules/karma/bin/karma start karma.conf.ts",
		"test-e2e-watch": "node --run pre-test && node --experimental-strip-types --disable-warning=ExperimentalWarning node_modules/karma/bin/karma start karma.conf.ts --auto-watch --no-single-run",
		"test-e2e-browserstack": "node --run pre-test && node --experimental-strip-types --disable-warning=ExperimentalWarning node_modules/karma/bin/karma start karma-browserstack.conf.ts",
		"prepare": "husky"
	},
